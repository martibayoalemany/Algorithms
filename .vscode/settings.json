{
    "java.configuration.updateBuildConfiguration": "automatic",
    "python.formatting.provider": "autopep8",
    "code-runner.executorMap": {
        "c": "gcc $fullFileName -lm && ./a.out",
        "go": "~/.local/share/umake/go/go-lang/bin/go $fileNameWithoutExt",
        "java": "cd $dir && javac -source 1.8 -target 1.8 -bootclasspath /usr/lib/jvm/java-8-openjdk-amd64/jre/lib/rt.jar $fileName && java $fileNameWithoutExt"                
        //"java": "cd $dir && javac -source 1.9 -target 1.9 $fileName && java  $fileNameWithoutExt"
    },
    
    "code-runner.cwd": "/home/username/src/src-custom/Algorithms/src/main/java",
    "code-runner.enableAppInsights": true,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.customCommand": "echo workspaceRoot: $workspaceRoot\n echo dir: $dir\n echo dirWithoutTrailingSlash: $dirWithoutTrailingSlash\n echo fullFileName: $fullFileName\n echo fileName: $fileName \n echo fileNameWithoutExt: $fileNameWithoutExt\n echo generic_cp: $workspaceRoot/src/main/java/generic\n",    
    "git.ignoreLimitWarning": true,
    "files.exclude": {
        ".java_8_src": true
    },
    "EndOfLineFormatting": {
    "regexpPattern" : "\\\\r\\\\n",
    "replaceChar" : "\n"
    }
}